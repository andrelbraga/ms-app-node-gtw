{
  "name": "phi-backoffice-gtw",
  "version": "1.0.0",
  "description": "Gateway phi backoffice",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start:prd": "node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/src/main/config/server.js",
    "start:dev": "tsnd --respawn --ignore-watch node_modules src/main/config/server",
    "start:coverage": "serve coverage/lcov-report",
    "test": "jest --passWithNoTests --runInBand --no-cache",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:ci": "jest --coverage",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:coveralls": "npm run test:ci && coveralls < coverage/lcov.info",
    "lint": "eslint \"src/**\" --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://git.4all.com/phi/development/services/backoffice/backoffice-gtw.git"
  },
  "keywords": [
    "phi",
    "bko",
    "gtw"
  ],
  "author": "AndrÃ© Braga",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "express": "^4.17.1",
    "joi": "^17.4.0",
    "module-alias": "^2.2.2",
    "swagger-ui-express": "^4.1.6",
    "typescript": "^4.2.3"
  },
  "devDependencies": {
    "@somosphi/logger": "^1.6.0",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.32",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.21.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "git-commit-msg-linter": "^3.0.0",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.3",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0"
  },
  "_moduleAliases": {
    "@": "src"
  }
}
